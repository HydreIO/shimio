{
  "name": "@hydre/shimio",
  "version": "2.1.0",
  "description": "A blazing fast websocket multiplexing curative therapy",
  "type": "module",
  "main": "src/index.js",
  "target": "esnext",
  "scripts": {
    "test": "node --harmony-top-level-await test/index.js",
    "coverage": "c8 --check-coverage node --harmony-top-level-await test/index.js",
    "lint": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --list-different && eslint --color .",
    "format": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --write",
    "postversion": "git push && git push --tags",
    "example": "node --harmony-top-level-await example/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/shimio.git"
  },
  "author": "Hydre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydreio/shimio/issues"
  },
  "keywords": [
    "shimio",
    "websocket",
    "multiplexing"
  ],
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "parserOpts": {
      "allowAwaitOutsideFunction": true
    },
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-numeric-separator"
    ]
  },
  "homepage": "https://github.com/hydreio/shimio#readme",
  "dependencies": {
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@hydre/doubt": "^6.1.0",
    "@sidy/eslint-config-sidy": "^1.4.10",
    "babel-eslint": "^11.0.0-beta.2",
    "c8": "^7.1.2",
    "eslint": "^7.1.0",
    "eslint-plugin-unicorn": "^20.0.0",
    "prettier-eslint-cli": "^5.0.0",
    "tap-spec-emoji": "^6.1.2"
  }
}
