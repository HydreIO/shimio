{
  "name": "@hydre/shimio",
  "version": "3.1.1",
  "description": "A minimal multiplexed Websocket server and client",
  "type": "module",
  "target": "esnext",
  "scripts": {
    "test": "node --harmony-top-level-await --harmony-private-methods test/index.js",
    "coverage": "c8 --check-coverage node --harmony-top-level-await --harmony-private-methods test/index.js",
    "lint": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --list-different && eslint --color .",
    "format": "prettier-eslint $PWD/\"**/*.js\" --print-width 60 --write",
    "postversion": "git push && git push --tags",
    "example": "node --harmony-top-level-await --harmony-private-methods example/index.js"
  },
  "exports": {
    "./client": "./src/Client.js",
    "./server": "./src/Server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/shimio.git"
  },
  "author": "Hydre",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hydreio/shimio/issues"
  },
  "keywords": [
    "shimio",
    "websocket",
    "server",
    "multiplexing"
  ],
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "parserOpts": {
      "allowAwaitOutsideFunction": true
    },
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-private-methods"
    ]
  },
  "homepage": "https://github.com/hydreio/shimio#readme",
  "dependencies": {
    "event-iterator": "gist:465b44a8e6c7e05dc8c173e809c08a12",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-numeric-separator": "^7.10.1",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@hydre/doubt": "^6.1.0",
    "@sidy/eslint-config-sidy": "^1.4.12",
    "babel-eslint": "^11.0.0-beta.2",
    "c8": "^7.1.2",
    "eslint": "^7.2.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "prettier-eslint-cli": "^5.0.0",
    "tap-spec-emoji": "^6.1.2"
  }
}
