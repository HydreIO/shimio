{
  "name": "@hydre/shimio",
  "version": "5.0.1",
  "description": "A minimal multiplexed Websocket server and client",
  "type": "module",
  "target": "esnext",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "node test/index.test.js",
    "coverage": "c8 --check-coverage node test/index.test.js",
    "lint": "prettier --check \"**/*.js\" && eslint .",
    "format": "prettier --write \"**/*.js\"",
    "postversion": "git push && git push --tags",
    "example": "node example/index.js"
  },
  "exports": {
    "./client": "./src/Client.js",
    "./server": "./src/Server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hydreio/shimio.git"
  },
  "author": "Hydre",
  "license": "UNLICENSE",
  "bugs": {
    "url": "https://github.com/hydreio/shimio/issues"
  },
  "keywords": [
    "shimio",
    "websocket",
    "server",
    "multiplexing"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/hydreio/shimio#readme",
  "dependencies": {
    "debug": "^4.3.7",
    "event-iterator": "^2.0.0",
    "events": "^3.3.0",
    "lru_map": "^0.4.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@hydre/doubt": "^7.1.0",
    "@sidy/eslint-config-sidy": "^1.4.15",
    "c8": "^10.1.3",
    "eslint": "^9.17.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "koa": "^2.15.3",
    "prettier": "^3.4.2",
    "tap-spec-emoji": "^6.1.2"
  }
}
